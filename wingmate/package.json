{
  "name": "wingmate",
  "displayName": "wingmate",
  "description": "僚机",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.82.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
		"onLanguage:typescript"
	],
  "main": "./out/extension.js",

  "scripts": {
    "vscode:prepublish": "pnpm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
