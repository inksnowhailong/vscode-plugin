{
  "name": "apidoc2ts",
  "displayName": "apidoc2ts",
  "description": "api接口文档解析到ts请求函数（持续开发中）",
  "version": "1.0.0",
  "author": {
    "name": "inksnow",
    "email": "inksnowhailong@gmail.com"
  },
  "publisher": "inksnow",
  "engines": {
    "vscode": "^1.70.0"
  },
  "icon": "./logo.png",
  "categories": [
    "Other"
  ],
  "pricing": "Free",
  "activationEvents": [
    "onLanguage:typescriptreact",
    "onLanguage:typescript",
    "onLanguage:vue",
    "onStartupFinished"
  ],
  "contributes": {
    "configuration": {
      "title": "apidoc2ts,自定义浏览器执行文件地址",
      "properties": {
        "apidoc2ts.exePath": {
          "type": "string",
          "default": "",
          "description": "谷歌浏览器或eage浏览器可执行文件地址"
        },
        "apidoc2ts.BaseResponse": {
          "type": "string",
          "default": "type BaseResponse<T> = {\n  code?: number\n  message?: string\n  data?: T\n  tid?: any\n  error?: boolean\n  success?: boolean\n}",
          "description": "返回值的基础结构数据类型，需要接受一个泛型，得到实际返回内容的数据类型"
        },
        "apidoc2ts.requestMethodContent": {
          "type": "string",
          "default": "import actionRequest from '@/utils/request';",
          "description": "请求函数的引用代码，每个api文件 会自动去引入一个基础的请求函数，推荐是你封装好的axios,并请默认导出一个函数，作为使用"
        },
        "apidoc2ts.apiFileMode": {
          "type": "string",
          "default": "api",
          "enum": [
            "api",
            "inline"
          ],
          "description": "设置api文件生成位置，默认值:api(生成在api目录)，可选值:inline:(当前执行命令文件所在文件夹);直接输入的自定义的绝对路径"
        }
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/inksnowhailong/vscode-plugin"
  },
  "files":[
    "**/*.png",
    "**/*.jpg",
    "**/*.gif",
    "images/*.png",
    "images/*.jpg",
    "images/*.gif"
  ],
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "pnpm run compile && pnpm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "build": " vsce package "
  },
  "devDependencies": {
    "@types/mocha": "^10.0.2",
    "@types/node": "18.x",
    "@types/vscode": "^1.70.0",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "@vscode/test-electron": "^2.3.4",
    "eslint": "^8.50.0",
    "glob": "^10.3.3",
    "mocha": "^10.2.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "axios": "^1.5.1",
    "dedent": "^1.5.1",
    "esbuild": "^0.19.4",
    "fuse.js": "^6.6.2",
    "puppeteer-core": "^21.3.6",
    "tabletojson": "^3.0.0",
    "yallist": "^4.0.0"
  }
}
